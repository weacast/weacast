{
  "name": "@weacast/gfs",
  "description": "GFS weather forecast model plugin for Weacast",
  "version": "1.6.0",
  "homepage": "https://github.com/weacast/weacast-gfs",
  "main": "lib/",
  "keywords": [
    "weacast",
    "weacast-plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/weacast/weacast-gfs.git"
  },
  "author": {
    "name": "Weacast contributors",
    "url": "https://github.com/weacast"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/weacast/weacast-gfs/issues"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "shx rm -rf lib/ && babel -d lib/ src/ -s",
    "watch": "npm run compile && babel --watch -d lib/ src/",
    "lint": "standard src/**/*.js test/**/*.js --fix",
    "mocha": "cross-env NODE_CONFIG_DIR=./test/config/ mocha --recursive test/ --exit --require babel-core/register",
    "test": "npm run compile && npm run lint && npm run mocha",
    "start": "npm run compile && node lib"
  },
  "standard": {
    "sourceType": "module",
    "env": [
      "mocha"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "@weacast/core": "^1.5.0",
    "@weacast/grib2json": "^1.0.3",
    "@feathersjs/errors": "^5.0.0-pre.16",
    "winston": "^3.6.0",
    "fs-extra": "^10.0.1",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.3.6",
    "chai-lint": "^0.1.1",
    "containerized": "^1.0.2",
    "cross-env": "^7.0.3",
    "feathers": "^2.1.2",
    "feathers-configuration": "^0.4.1",
    "mocha": "^9.2.2",
    "shx": "^0.3.4",
    "standard": "^16.0.4"
  },
  "snyk": true
}
